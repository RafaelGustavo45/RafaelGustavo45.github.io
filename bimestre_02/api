<script> 
    function chamar_api(){
        fetch('http://shibe.online/api/shibes?count=1&urls=true&httpsUrls=true')
          .then(response => response.json())
          .then(data => exibir_imagem(data));
       }
    
    function exibir_imagem(data){
        document.getElementById("img").src = data[0];
    }
    
    </script> 
